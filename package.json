    "@babel/preset-typescript": "^7.3.3",
    "@types/node": "^10.17.6",
    "@types/prettier": "^1.16.4",
    "@types/react": "^16.8.23",
    "@types/sass": "^1.16.0",
    "@types/webpack": "^4.4.34",
